{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",  // tabbar首页
			"style": {
				"navigationStyle": "custom",//navigationStyle设置为custom去除原生导航栏。
				"onReachBottomDistance": 50
			}
		}
	    ,{
            "path" : "pages/categray/categray",  // tabbar分类
            "style" : {
				"navigationBarTitleText": "分类"
			}
        }
        ,{
            "path" : "pages/cart/cart",  // tabbar购物车
            "style" : {
				"navigationBarTitleText": "购物车"
			}
        }
        ,{
            "path" : "pages/home/home",
            "style" : {
				"navigationBarTitleText": "个人中心"
			}
        }
    ],
	//全局设置
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "买菜",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#42c961"
	},
	"tabBar": {
	    "color": "#7A7E83",
	    "selectedColor": "#3cc51f",
	    "borderStyle": "black",
	    "backgroundColor": "#ffffff",
	    "list": [{
	        "pagePath": "pages/index/index",
	        "iconPath": "static/tabbar/h1.png",
	        "selectedIconPath": "static/tabbar/h2.png",
	        "text": "首页"
	    }, {
	        "pagePath": "pages/categray/categray",
	        "iconPath": "static/tabbar/c1.png",
	        "selectedIconPath": "static/tabbar/c2.png",
	        "text": "分类"
	    },
		{
	        "pagePath": "pages/cart/cart",
	        "iconPath": "static/tabbar/g1.png",
	        "selectedIconPath": "static/tabbar/g2.png",
	        "text": "购物车"
	    }, {
	        "pagePath": "pages/home/home",
	        "iconPath": "static/tabbar/i1.png",
	        "selectedIconPath": "static/tabbar/i2.png",
	        "text": "我的"
	    }]
	}
}
